
#
# SoC configuration
#

#
# W80X
#

#
# XT804
#
CONFIG_FLASH_BASE_ADDR=0x8000000
CONFIG_SRAM_BASE_ADDR=0x20000000
CONFIG_SRAM_LENGTH=0x28000
CONFIG_DRAM_BASE_ADDR=0x20028000
CONFIG_DRAM_LENGTH=0x20000
CONFIG_PSRAM_BASE_ADDR=0x30000000
CONFIG_PSRAM_MAX_LENGTH=0x800000
CONFIG_UART_BASE_ADDR=0x40010600
CONFIG_UART_LENGTH=0xA00
CONFIG_XT804_SEPARATE_IRQ_SP=y
CONFIG_XT804_ARCH_INTERRUPTSTACK=4096
CONFIG_XT804_IRQ_VECTOR_SIZE=256
CONFIG_XT804_SAVE_VR_REGISTERS=y
CONFIG_XT804_SAVE_HIGH_REGISTERS=y
CONFIG_XT804_SYSTEM_SECURE=y
CONFIG_XT804_TRAP_PRINT_STR=y
CONFIG_XT804_TRAP_OUTPUT_RAMDUMP=y
# end of XT804

CONFIG_W80X_BASIC_DRIVER=y

#
# Toolchain
#

#
# Path to the toolchain, see help for details
#
CONFIG_TOOLCHAIN_PATH=""

#
# Toolchain name prefix, see help for details
#
CONFIG_TOOLCHAIN_PREFIX="csky-elfabiv2-"
# end of Toolchain

CONFIG_BUILD_TYPE_W800=y
