CONFIG_CHIP_FAMILY_NAME="w80x"
CONFIG_CHIP_HAVE_RF=y
CONFIG_CHIP_HAVE_WIFI=y
CONFIG_CHIP_HAVE_BT=y
CONFIG_BUILD_VERSION="2.4-rc"
